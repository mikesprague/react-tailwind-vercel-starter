    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "homepage": "",
  "license": "MIT",
  "dependencies": {
    "@tailwindcss/vite": "4.1.12",
    "@tippyjs/react": "4.2.6",
    "cross-env": "10.0.0",
    "daisyui": "5.0.54",
    "dayjs": "1.11.15",
    "dotenv": "17.2.1",
    "prop-types": "15.8.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-icons": "5.5.0",
    "swr": "2.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
