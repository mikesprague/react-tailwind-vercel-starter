    "axios": "1.6.2",
    "cross-env": "7.0.3",
    "daisyui": "4.4.20",
    "dayjs": "1.11.10",
    "dotenv": "16.3.1",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.12.0",
    "swr": "2.2.4"
  },
  "devDependencies": {
