{
  "name": "react-tailwindcss-vercel-starter",
  "version": "0.1.0",
  "description": "Template for a barebones Vercel hosted React/Tailwind CSS PWA with an API endpoint powered by a serverless function",
  "private": true,
  "engines": {
    "node": ">= 22.x",
    "npm": ">= 10.x"
  },
  "packageManager": "npm@11.8.0",
  "type": "module",
  "scripts": {
    "build": "npx vite build",
    "dev": "npx vite serve --port $PORT",
    "preview": "vite preview",
    "start": "cross-env NODE_ENV=development npx vercel dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "repository": {
    "url": "https://github.com/mikesprague/react-tailwindcss-vercel-starter"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "homepage": "",
  "license": "MIT",
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@tippyjs/react": "^4.2.6",
    "cross-env": "^10.1.0",
    "daisyui": "^5.5.18",
    "dayjs": "^1.11.19",
    "dotenv": "^17.3.1",
    "prop-types": "^15.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0",
    "swr": "^2.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.15",
    "@tailwindcss/typography": "^0.5.19",
    "@vitejs/plugin-react-swc": "^4.2.3",
    "cz-git": "^1.12.0",
    "stylelint": "^17.2.0",
    "stylelint-config-standard": "^40.0.0",
    "tailwindcss": "^4.1.18",
    "vercel": "^50.15.1",
    "vite": "^7.3.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-pwa": "^1.2.0"
  },
  "overrides": {
    "undici": "^7.20.0",
    "tar": "^7.5.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "volta": {
    "node": "24.13.0",
    "npm": "11.8.0"
  }
